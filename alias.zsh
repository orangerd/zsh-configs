################################################################################
# alias.zsh
################################################################################
# ls alternative
alias l="ls -a"
alias ll="ls -l"
alias la="ls -la"

# vim alias
alias vi="`which vim`"

# Push/Pop/Dirs alias
alias pu="pushd"
alias po="popd"

# Overrides for rm, cp, mv
alias rm='rm'
alias cp='cp'
alias mv='mv'

